<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layoutScreen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    android:orientation="vertical" >

    <vn.com.sonca.SetttingApp.TopSettingView
        android:id="@+id/TopSettingView"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="0.1" />
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="20dp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="0.9"
        android:orientation="horizontal"
        android:weightSum="10" >

        <View
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="0.2" />
        
        <LinearLayout
            android:id="@+id/layoutLeft"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="3"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <vn.com.sonca.SettingAll.MyTextSettingView
                android:id="@+id/myTextSettingViewA1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SettingAll.PopSettingView
                android:id="@+id/popSettingViewA1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp"                
                android:visibility="gone" />

            <TextView
                android:id="@+id/text_pop_a1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text=""
                android:textColor="#B4FEFF"
                android:textSize="16sp"
                android:visibility="gone" />
            
            <vn.com.sonca.SetttingApp.ButonOnOff
                    android:id="@+id/youtubeCommand"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            
            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <vn.com.sonca.SettingAll.MyTextSettingView
                android:id="@+id/myTextSettingViewA3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SettingAll.PopSettingView
                android:id="@+id/popSettingViewA3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <TextView
                android:id="@+id/text_pop_a3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text=""
                android:textColor="#B4FEFF"
                android:textSize="14sp" />
            
            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <vn.com.sonca.SettingAll.MyTextSettingView
                    android:id="@+id/myTextSettingView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <vn.com.sonca.SettingAll.PopSettingView
                    android:id="@+id/popSettingView0"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <TextView
                    android:id="@+id/text_pop_0"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="@string/setting_pop_3"
                    android:textColor="#B4FEFF"
                    android:textSize="16sp" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <LinearLayout
                android:id="@+id/layouSwitch"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <vn.com.sonca.SettingAll.MyTextSettingView
                    android:id="@+id/myTextSettingView0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <vn.com.sonca.SetttingApp.ItemTimerView
                    android:id="@+id/button_timer_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <vn.com.sonca.SetttingApp.ItemTimerView
                    android:id="@+id/button_timer_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp" />

                <vn.com.sonca.SetttingApp.ItemTimerView
                    android:id="@+id/button_timer_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp" />

                <vn.com.sonca.SetttingApp.ItemTimerView
                    android:id="@+id/button_timer_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp" />

                <vn.com.sonca.SetttingApp.ItemTimerView
                    android:id="@+id/button_timer_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp" />

                <View
                    android:layout_width="fill_parent"
                    android:layout_height="10dp" />

                <TextView
                    android:id="@+id/text_timer_line_1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="@string/text_change_sleep_7"
                    android:textColor="#B4FEFF"
                    android:textSize="16sp" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="0dp"
                android:layout_weight="0.1" />
        </LinearLayout>
        
        <View
            android:id="@+id/viewLine2"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="@drawable/touch_shape_line_ver" />

        <LinearLayout
            android:id="@+id/layoutCenter"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="2.8"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <vn.com.sonca.SettingAll.MyTextSettingView
                android:id="@+id/myTextSettingViewA2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SettingAll.PopSettingView
                android:id="@+id/popSettingViewA2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <TextView
                android:id="@+id/text_pop_a2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text=""
                android:textColor="#B4FEFF"
                android:textSize="14sp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />
            
            <LinearLayout
                android:id="@+id/layoutPopup"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <vn.com.sonca.SetttingApp.MyTextSettingView
                    android:id="@+id/myTextSettingView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <vn.com.sonca.SetttingApp.PopSettingView
                    android:id="@+id/popSettingView"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="1dp" />

                <TextView
                    android:id="@+id/text_pop_1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:text="@string/setting_pop_3"
                    android:textColor="#B4FEFF"
                    android:textSize="14sp" />
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <vn.com.sonca.SettingAll.MyTextSettingView
                android:id="@+id/myTextSettingViewA4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SettingAll.PopSettingView
                android:id="@+id/popSettingViewA4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <TextView
                android:id="@+id/text_pop_a4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text=""
                android:textColor="#B4FEFF"
                android:textSize="14sp" />
            
            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <vn.com.sonca.SettingAll.MyTextSettingView
                android:id="@+id/myTextSettingViewA5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SettingAll.PopSettingView
                android:id="@+id/popSettingViewA5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <TextView
                android:id="@+id/text_pop_a5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:text=""
                android:textColor="#B4FEFF"
                android:textSize="14sp" />
            
            <View
                android:id="@+id/viewSpace1"
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D" />

            <vn.com.sonca.SetttingApp.MyTextSettingView
                android:id="@+id/myTextSettingView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SetttingApp.ItemScreenView
                android:id="@+id/button_screen_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SetttingApp.ItemScreenView
                android:id="@+id/button_screen_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp" />
            
            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp" />

            <TextView
                android:id="@+id/text_timer_screen_1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:gravity="center_horizontal"
                android:text="@string/setting_screen_1"
                android:textColor="#FFFFFF"
                android:textSize="14sp" />

            <View
                android:id="@+id/viewSpace2"
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="#01192D"
                android:visibility="gone" />

            <vn.com.sonca.SetttingApp.MyTextSettingView
                android:id="@+id/myTextSettingViewNew"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp"
                android:visibility="gone" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_marginTop="1dp"
                android:layout_weight="0.2"
                android:orientation="horizontal"
                android:visibility="gone" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.1" />

                <vn.com.sonca.Touch.CustomView.DownVideoView
                    android:id="@+id/downVideoView"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.4" />

                <vn.com.sonca.Touch.CustomView.XoaVideoView
                    android:id="@+id/xoaVideoView"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.4" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.1" />
            </LinearLayout>

            <View
                android:layout_width="1dp"
                android:layout_height="0dp"
                android:layout_weight="0.2" />
        </LinearLayout>

        <View
            android:id="@+id/viewLine1"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="@drawable/touch_shape_line_ver" />

        <LinearLayout
            android:id="@+id/layoutRight"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="4"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <vn.com.sonca.SetttingApp.MyTextSettingView
                android:id="@+id/myTextSettingView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="1dp" />

            <vn.com.sonca.SetttingApp.AppInfoView
                android:id="@+id/appInfoView"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>